<div class="row">
    <div class="col-xs-12"><h2>App Permissions</h2></div>
</div>
<div class="row" ng-repeat="appPermission in activeApp.appPermissions track by $index">

    <div class="col-xs-12">
        <input type="text" ng-model="appPermission.appUser"/>
    </div>
    <div class="col-xs-12">
        <div class="col-sm-4 col-xs-12">Read</div>
        <div class="col-sm-4 col-xs-12">Write</div>
        <div class="col-sm-4 col-xs-12">Execute</div>
    </div>
    <div class="col-xs-12">
        <div class="col-sm-4 col-xs-12"><input type="checkbox" ng-model="appPermission.read"/></div>
        <div class="col-sm-4 col-xs-12"><input type="checkbox" ng-model="appPermission.write"/></div>
        <div class="col-sm-4 col-xs-12"><input type="checkbox" ng-model="appPermission.execute"/></div>
    </div>
</div>
<div class="row">
    <div class="col-xs-12">
        Save
    </div>
</div>