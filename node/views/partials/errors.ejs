
<% if(messages.errors && messages.errors.length) {
%>
<div class="row">
<div class="col-xs-0 col-sm-4"></div>
<div class="col-xs-12 col-sm-4 error-box">
    <div class="col-xs-12">
        <h3>Error</h3>
        <ul>
            <% messages.errors.forEach(function(error){%>
            <li><%=error %></li>
            <% })%>
        </ul>
    </div>
</div>
<div class="col-xs-0 col-sm-4"></div>
</div>
<%}%>