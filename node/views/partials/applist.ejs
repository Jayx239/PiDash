<div class="app-list col-xs-12">
    <div class="row">
        <div class="col-xs-12"><h2>Apps</h2></div>
    </div>
    <div class="row" ng-repeat="piDashApp in piDashApps track by $index">
        <div class="col-xs-12 app-row" ng-click="setActiveApp(piDashApp.app.appId);">
            <h4>{{piDashApp.app.name}}</h4>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12 app-row">
            <button ng-click="addApplication()" class="col-xs-12 btn btn-primary">Add App</button>
        </div>
    </div>
</div>